{
    "manifest_version": 2,
    "name": "ChatGPT Chat Print",
    "version": "1.0",
    "description": "A Chrome extension for printing ChatGPT conversations",
    "permissions": [
      "activeTab", "scripting"
    ],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  